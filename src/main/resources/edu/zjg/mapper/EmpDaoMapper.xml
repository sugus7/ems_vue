<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.zjg.dao.EmpDAO">
    <select id="findAll" resultType="edu.zjg.entity.Emp">
      select id,name,path,salary,age from t_emp
    </select>
    <insert id="save" parameterType="edu.zjg.entity.Emp" useGeneratedKeys="true" keyProperty="id">
        insert into t_emp values(#{id},#{name},#{path},#{salary},#{age})
    </insert>
    <delete id="delete" parameterType="String">
        delete from t_emp where id=#{id}
    </delete>
    <select id="findOne" parameterType="String" resultType="edu.zjg.entity.Emp">
         select id,name,path,salary,age from t_emp where id=#{id}
    </select>
</mapper>